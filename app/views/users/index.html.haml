.container
  .container-fluid
    .row
      .content-main
        %section.col-sm-10.col-xs-offset-1.main-content
          / 検索user一覧
          .do_list
            %h4
              %i.fa.fa-tags{"aria-hidden" => "true"}
              「Masaaki」ユーザー一覧
            - @users.each do |user|
              %div
                = link_to user_list_items_path(user.id, 1)  do
                  %img.img-circle{:height => "50", :src => "/plofile_fb_n.png", :width => "50"}/
                  %span= user.name
